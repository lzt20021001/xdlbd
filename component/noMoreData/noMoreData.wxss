.noMoreData_hint{
  width: 100%;
  height: 225rpx;
  line-height: 225rpx;
  display: flex;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:#BDBDBD;
  background-color: #F5F5FA;
}
.noMoreData_hint view:first-child{
  flex: 1;
  position: relative;
}
.noMoreData_hint view:last-child{
  flex: 1;
  position: relative;
}
.noMoreData_hint view text{
  position: absolute;
  width:65rpx;
  height:1rpx;
  background:rgba(204,204,204,1);
  opacity:0.7;
  top:110rpx;
  right:28rpx;
}
.noMoreData_hint view:last-child text{
  left: 28rpx;
}

.noMoreData_goTop{
  position: absolute !important;
  width:88rpx;
  height:88rpx;
  background:linear-gradient(267deg,rgba(85,235,255,1) 0%,rgba(28,145,242,1) 100%);
  box-shadow:0px 5rpx 40rpx 0px rgba(0,155,223,0.2);
  border-radius:50%;
  right: 30rpx;
  top:50%;
  margin-top: -50rpx;
}
.noMoreData_goTop image{
  position: absolute;
  width: 34rpx;
  height: 19rpx;
  left:50%;
  top:50%;
  margin-top: -10rpx;
  margin-left: -17rpx;
}