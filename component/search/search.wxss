
.search{
  flex: 1;
  height:63rpx;
  line-height: 63rpx;
  background:rgba(245,245,250,1);
  border-radius:32px;
  margin-top: 21rpx;
  position: relative;
}
.search_img{
  position: absolute;
  width: 26rpx;
  height: 26rpx;
  left:22rpx;
  top:19rpx;
}
.search_del_img {
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  right:22rpx;
  top:5rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 2;
}
.search_del_img image {
  width: 26rpx;
  height: 26rpx;
}
.delKeywordview{
  position: absolute;
  width: 80rpx;
  height: 64rpx;
  top:0;
  right:0;
  text-align: right;
  z-index: 10;
 
}
.delKeyword_img {
   width: 25rpx;
   height: 25rpx;
   margin-right: 20rpx;
}
.search input{
  font-size:26rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  height:63rpx;
  line-height: 63rpx;
  padding:0 60rpx 0 60rpx;
  box-sizing: border-box;
}
.search_place{
  font-size:26rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(204,204,204,1);
}
.screen{
  padding: 0 30rpx 0 28rpx;
  font-size:28rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  text-decoration:underline;
  color:rgba(28,158,242,1);
  width:90rpx;
  position: relative;
}
.high-screen {
  width: 196rpx;
  padding: 0;
  text-decoration: none;
  color: #333333;
  font-size: 28rpx;
}
.screenW {
  width: auto;
}
.screen image {
  width:23rpx;
  height: 26rpx;
}
.screen .image{
  position: absolute;
  width:23rpx;
  height: 26rpx;
  top:50%;
  right: 30rpx;
  margin-top: -12rpx;
}
.screen .high-image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.trigger image{
  width: 29rpx;
  height: 23rpx;
  top:50%;
  left:25rpx;
  margin-top: -10rpx;
}
.trigger {
  text-align: right; 
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.header{
  width:100%;
  height: 104rpx;
  line-height: 104rpx;
  padding-left:30rpx;
  box-sizing: border-box;
  display: flex;
  background: #fff;
}

.line{
  width: 100%;
  height: 16rpx;
  background-color: #F5F5FA;
}
.fx-center {
  display: flex;
  justify-content: center;
  align-items: center;
}