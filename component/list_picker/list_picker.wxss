

.list{
  width: 100%;
  min-height: 99rpx;
  line-height: 99rpx;
  border-bottom: 1rpx solid rgba(230,230,230,.7);
  display: flex;
  padding-left: 30rpx;
  box-sizing: border-box;
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:#666;

}
.list_title{
  padding-right: 20rpx;
  box-sizing: border-box;
}
.titleColor{
  font-size:23rpx;
  font-weight:500;
  color:#F7517F;
  margin-right: 15rpx;
}
.list_picker{
  flex: 1;
  text-align: right;
  position: relative;
  padding-right: 40rpx;
  box-sizing: border-box;
}
.selImg{
  position: absolute;
  top:50%;
  right: 30rpx;
  width: 13rpx;
  height: 20rpx;
  margin-top: -10rpx;
}
.blueColor {
  color: #1C9EF2 !important;
}
.icon_img{
  position: absolute;
  top:50%;
  right: 30rpx;
  width: 20rpx;
  height:28rpx;
  transform: translateY(-50%);
}
.search{
  position: absolute;
  top:50%;
  right: 30rpx;
  width: 26rpx;
  height: 28rpx;

  transform: translateY(-50%);
}

.list_picker_input{
  flex: 1;
  text-align: right;
}

.list_input{
  border: 0;
  width: 100%;
  height: 99rpx;
  line-height: 99rpx;
  font-size:30rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(51,51,51,1);
  padding-right: 30rpx;
  box-sizing: border-box;
}
.list_address{
  flex: 1;
  position: relative;
  padding-right: 40rpx;
  box-sizing: border-box;
}
.address{
  width: 100%;
  padding:32rpx 30rpx 28rpx 0;
  box-sizing: border-box;
  line-height: 36rpx;
  text-align: right;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.input_holder{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.list_picker_btn{
  position: relative;
  flex: 1;
  text-align: right;
  padding-right: 30rpx;
  box-sizing: border-box;
}
.showStyle{
  font-size:28rpx;
  font-family:PingFangSC-Medium;
  /* font-weight:500; */
  color:rgba(51,51,51,1);
  padding-right: 30rpx;
  box-sizing: border-box;
  height: 99rpx;
  line-height: 99rpx;
  border: 0;
}
.showStyle.pad{
  padding-right: 40rpx;
}



.process{
  flex: 1;
  display: flex;
  padding-right:20rpx; 
}
.process_sel{
  font-size: 30rpx;
  line-height: 30rpx;
  height: 30rpx;
  padding:35rpx 25rpx 35rpx 40rpx;
}

.process_sel:first-child{
  flex: 1;
  padding: 0;
}
.sel{
  display: inline-block;
  width:30rpx !important;
  height:30rpx;
  margin-right: 10rpx;
  box-sizing: border-box;
}
/* form表单获取name值 */
.getName{
  width:0 !important;
  height:0 !important;
  min-height: 0 !important;
  max-height: 0 !important;
}